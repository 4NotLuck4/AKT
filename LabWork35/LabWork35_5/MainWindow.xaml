<Window x:Class="LabWork35_5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LabWork35_5"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Button x:Name="PrintButton" Content="Print" Click="PrintButton_Click"/>
        <Border>
            <Canvas x:Name="PrintContainer">
                <Ellipse Canvas.Left="50" Canvas.Top="50" Width="100" Height="100" Fill="LightGoldenrodYellow"/>
                <TextBlock Canvas.Left="70" Canvas.Top="90" FontSize="16" Text="Print text"/>
            </Canvas>
        </Border>
    </StackPanel>
</Window>
